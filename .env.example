# .env.example - Copy this file to .env and fill in your actual values
# DO NOT commit .env file to git!

# --- Database Configuration (Login Credentials) ---
DB_HOST=your_mysql_host.aivencloud.com
DB_PORT=21934
DB_USER=your_db_user
DB_PASSWORD=your_db_password_here
DB_DATABASE=Login_Credentials

# --- JWT Configuration ---
SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=720

# --- Application-wide API Keys ---
ADMIN_API_KEY=your_secure_api_key_here

# --- Database Configuration (DBT Cases) ---
DBT_DB_HOST=your_mysql_host.aivencloud.com
DBT_DB_PORT=21934
DBT_DB_USER=your_db_user
DBT_DB_PASSWORD=your_db_password_here
DBT_DB_DATABASE=defaultdb
